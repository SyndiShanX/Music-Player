icon = b''
small_icon = b''
playButton = b''
pauseButton = b''
prevButton = b''
nextButton = b''
shuffleButton = b''
shuffleEnabledButton = b''
repeatButton = b''
repeatEnabledButton = b''
settingsButton = b''
